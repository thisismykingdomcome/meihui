[is="components/tabbar-item/index"] {
    flex: 1;
}

page {
    background-color: #f2f7fa;
}

.page__hd {
    padding: 40px;
}

.page__bd {
    padding-bottom: 40px;
}

.page__bd_spacing {
    padding-left: 15px;
    padding-right: 15px;
}

.page__ft {
    padding-bottom: 10px;
    text-align: center;
}

.page__title {
    text-align: left;
    font-size: 20px;
    font-weight: 400;
}

.page__desc {
    margin-top: 5px;
    color: #888;
    text-align: left;
    font-size: 14px;
}

.zxsite-bottom-placeholder {
    width: 100%;
    height: 134rpx;
    position: relative;
    bottom: 0;
    z-index: -1;
}

.zxsite-bottom {
    display: flex;
    overflow: hidden;
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    padding: 10rpx 0;
    background-color: #fff;
    z-index: 999999;
}

.zxsite-bottom.black:after {
    content: '';
    position: absolute;
    top: 0;
    background: #ccc;
    width: 100%;
    height: 1px;
    transform: scaleY(0.5);
    transform-origin: 0 0;
}

.zxsite-bottom.active {
    background-color: #fff;
}

.zxsite-bottom.active navigatorurl {
    background-color: #fff;
}

.zxsite-bottom .zxsite-bottom-item {
    flex: 1;
    padding: 10rpx;
    text-align: center;
}

.zxsite-bottom .item-img {
    width: 50rpx;
    height: 50rpx;
    display: inline-block;
}

.zxsite-bottom .item-text {
    display: block;
    font-size: 12px;
    line-height: 1;
}

.goHome {
    position: fixed;
    bottom: 150rpx;
    right: 30rpx;
    width: 50rpx;
    height: 50rpx;
    background-color: #1ba9ba;
    padding: 30rpx;
    border-radius: 50%;
}

.goHome>image {
    width: 100%;
}

.text-right {
    text-align: right;
}

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
}

.wxParse-img {
    display: block!important;
}

page {
    background-color: #f8f8f8;
}

.loading {
    position: fixed;
    height: 100%;
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    background-color: #f8f8f8;
    z-index: 9999;
}

.loading image {
    width: 50px;
    height: 50px;
}