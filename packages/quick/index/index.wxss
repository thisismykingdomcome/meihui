.goods {
    display: flex;
    position: absolute;
    width: 100%;
    top: 106px;
    bottom: 46px;
    overflow: hidden;
    font-family: "微软雅黑";
}

.goods .menu-wrapper {
    flex: 0 0 190rpx;
    width: 190rpx;
    background: #f8f8f8;
}

.goods .menu-wrapper .menu-item {
    display: table;
    width: 140rpx;
    min-height: 54px;
    line-height: 14px;
    padding: 0 25rpx;
    background-color: #f8f8f8;
}

.goods .menu-wrapper .menu-item.current {
    background: #fff;
}

.goods .menu-wrapper .menu-item .icon {
    display: inline-block;
    width: 12px;
    height: 12px;
    vertical-align: top;
    margin-right: 4px;
    background-size: 12px 12px;
    background-repeat: no-repeat;
}

.goods .menu-wrapper .menu-item .text {
    display: table-cell;
    width: 140rpx;
    vertical-align: middle;
    position: relative;
    font-size: 14px;
    line-height: 25px;
    padding: 10px 0;
}

.goods .goods-wrapper {
    flex: 1;
    background-color: #fff;
}

.goods .goods-wrapper .goods-list .title {
    padding-left: 14px;
    height: 26px;
    line-height: 26px;
    font-size: 12px;
    color: rgb(147,153,159);
    background: #f8f8f8;
}

.goods .goods-wrapper .goods-list .goods-item {
    display: flex;
    margin: 18px;
    padding-bottom: 18px;
    position: relative;
    border-bottom: 1px solid rgba(7,17,27,0.1);
}

.goods .goods-wrapper .goods-list .goods-item:last-child {
    border: none;
    margin-bottom: 0px;
}

.goods .goods-wrapper .goods-list .goods-item .icon {
    flex: 0 0 57px;
    margin-right: 10px;
}

.goods .goods-wrapper .goods-list .goods-item .content {
    flex: 1;
}

.goods .goods-wrapper .goods-list .goods-item .content .name {
    margin: 2px 0 8px 0;
    font-size: 14px;
    line-height: 14px;
    height: 14px;
    color: rgb(7,17,27);
    text-overflow: ellipsis;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.goods .goods-wrapper .goods-list .goods-item .content .desc,.extra {
    font-size: 10px;
    line-height: 10px;
    color: rgb(147,153,159);
}

.goods .goods-wrapper .goods-list .goods-item .content .desc {
    margin-bottom: 8px;
    line-height: 12px;
}

.goods .goods-wrapper .goods-list .goods-item .content .extra .count {
    margin-right: 12px;
}

.goods .goods-wrapper .goods-list .goods-item .content .price {
    font-weight: 700px;
    line-height: 24px;
}

.goods .goods-wrapper .goods-list .goods-item .content .price .now {
    margin-right: 8px;
    font-size: 14px;
    color: rgb(240,20,20);
}

.goods .goods-wrapper .goods-list .goods-item .content .price .old {
    font-size: 10px;
    color: rgb(147,153,159);
    text-decoration: line-through;
}

.goods .goods-wrapper .goods-list .goods-item .content .cart-control-wrapper {
    position: absolute;
    right: 0;
    bottom: 12px;
}

.cart-control-wrapper .cart-control {
    font-size: 0;
}

.cart-control-wrapper .cart-control .cart-count {
    font-size: 14px;
    line-height: 34px;
    text-align: center;
}

.cart-control-wrapper .cart-control .cart-reduce {
    width: 24px;
    height: 24px;
    margin: 6px;
    background-image: url(data:image/jpeg;base64,);
    background-size: 100% 100%;
}

.cart-control-wrapper .cart-control .cart-add {
    width: 24px;
    height: 24px;
    margin: 6px;
    background-image: url(data:image/jpeg;base64,);
    background-size: 100% 100%;
}

.shop-cart {
    position: fixed;
    left: 0px;
    bottom: 0px;
    z-index: 50;
    width: 100%;
    height: 48px;
    background-color: #fff;
}

.shop-cart .content {
    display: flex;
    box-shadow: 0 -2px 10px 0 rgba(0,0,0,0.1);
}

.shop-cart .content .content-left {
    flex: 1;
}

.shop-cart .content .content-left .cart-wrapper {
    display: inline-block;
    position: relative;
    top: -10px;
    margin: 0 12px;
    width: 50px;
    height: 50px;
    box-sizing: border-box;
    vertical-align: top;
    border-radius: 50%;
}

.shop-cart .content .content-left .cart-wrapper .number {
    position: absolute;
    top: 0;
    right: 0;
    width: 16px;
    height: 16px;
    line-height: 16px;
    text-align: center;
    border-radius: 16px;
    font-size: 9px;
    font-weight: 700;
    background-color: #fff;
    border: 1px #ff4444 solid;
    color: #ff4444;
}

.shop-cart .content .content-left .cart-wrapper .icon-cart {
    width: 100%;
    height: 100%;
    text-align: center;
    border-radius: 50%;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
    box-shadow: 0 0 10px rgba(0,0,0,0.4);
}

.shop-cart.no-empty .content .content-left .cart-wrapper .icon-cart {
    background-image: url(data:image/png;base64,);
    box-shadow: 0 0 10px rgba(255,68,68,0.4);
}

.shop-cart .content .content-left .price {
    display: inline-block;
    vertical-align: top;
    margin-top: 12px;
    line-height: 24px;
    box-sizing: border-box;
    font-size: 24rpx;
    color: #999999;
}

.shop-cart .content .content-left .desc {
    display: inline-block;
    vertical-align: top;
    line-height: 24px;
    margin-left: 12px;
    margin-top: 12px;
    color: rgba(255,255,255,0.4);
    font-size: 10px;
}

.shop-cart .content .content-right {
    width: 328rpx;
}

.shop-cart .content .content-right .pay {
    height: 48px;
    line-height: 48px;
    text-align: center;
    font-size: 28rpx;
    color: #a3a3a3;
    background: #efefef;
}

.shop-cart .content .content-right.enough {
    width: 230rpx;
}

.shop-cart .content .content-right.enough .pay {
    font-size: 30rpx;
    background: #ff4444;
    color: #fff;
}

.shop-cart .ball-container .ball {
    position: fixed;
    left: 32px;
    bottom: 22px;
    z-index: 200;
    transition: all 0.6s cubic-bezier(0.49,-0.29,0.75,0.41);
}

.shop-cart .ball-container .ball .inner {
    width: 16px;
    height: 16px;
    border-radius: 50%;
    background: rgb(0,160,220);
    transition: all 0.4s linear;
}

.shop-cart .shop-cart-list {
    position: absolute;
    top: 0;
    left: 0;
    z-index: -1;
    width: 100%;
    transform: translate3d(0,-100%,0);
    padding-bottom: 10px;
    background-color: #fff;
}

.shop-cart .shop-cart-list :fade-enter-active,:fade-leave-active {
    transition: all 0.5s transform translate3d(0,-100%,0);
}

.shop-cart .shop-cart-list :fade-enter,:fade-leave-active {
    transform: translate3d(0,0,0);
}

.shop-cart .shop-cart-list .list-header {
    height: 40px;
    line-height: 40px;
    padding: 0 18px;
    border-bottom: 1px solid rgba(7,17,27,0.1);
    border-left: 4px solid #ff4444;
}

.shop-cart .shop-cart-list .list-header .title {
    float: left;
    font-size: 14px;
    color: rgb(7,17,27);
}

.shop-cart .shop-cart-list .list-header .empty {
    float: right;
    font-size: 12px;
    color: #999;
}

.shop-cart .shop-cart-list .list-content {
    padding-left: 18px;
    max-height: 217px;
    background: #fff;
    overflow-y: auto;
}

.shop-cart .shop-cart-list .list-content .shop-cart-goods {
    display: flex;
    padding: 10px 18px 10px 0;
    line-height: 36px;
    font-size: 14px;
    color: rgb(7,17,27);
    justify-content: space-between;
}

.shop-cart .shop-cart-list .list-content .shop-cart-goods:not(:first-child) {
    border-top: 1px solid rgba(7,17,27,0.1);
}

.shop-cart .shop-cart-list .list-content .shop-cart-goods .title {
    width: 350rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.shop-cart .shop-cart-list .list-content .shop-cart-goods .sub-title {
    font-size: 12px;
    line-height: normal;
    color: #999;
    text-overflow: ellipsis;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.header {
    position: relative;
    color: #000;
    overflow: hidden;
    background: #f8f8f8;
}

.header .content-wrapper {
    display: flex;
    position: relative;
    padding: 24px 12px 18px;
}

.header .content-wrapper .content {
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    font-size: 14px;
    margin-left: 16px;
}

.header .content-wrapper .content .title {
    font-size: 16px;
    line-height: 18px;
    font-weight: bold;
}

.header .content-wrapper .content .description {
    color: #999;
    font-size: 12px;
    vertical-align: top;
    text-overflow: ellipsis;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.header .content-wrapper .content .support .icon {
    display: inline-block;
    width: 12px;
    height: 12px;
    vertical-align: top;
    margin-right: 4px;
    background-size: 12px 12px;
    background-repeat: no-repeat;
}

.header .content-wrapper .content .support .text {
    line-height: 12px;
    font-size: 12px;
}

.header .content-wrapper .content .supports-count {
    position: absolute;
    right: 12px;
    bottom: 14px;
    padding: 0 8px;
    height: 24px;
    line-height: 24px;
    border-radius: 12px;
    background: rgba(0,0,0,0.2);
    text-align: content;
}

.header .content-wrapper .content .supports-count .count {
    font-size: 10px;
    vertical-align: top;
}

.header .content-wrapper .content .supports-count .icon {
    margin-left: 2px;
    line-height: 24px;
    font-size: 10px;
}

.header .bulletin-wrapper {
    position: relative;
    background-color: rgba(7,17,27,0.2);
    height: 28px;
    line-height: 28px;
    padding: 0 22px 0 12px;
    white-space: normal;
    overflow: hidden;
    text-overflow: ellipsis;
}

.header .bulletin-wrapper .bulletin-title {
    display: inline-block;
    vertical-align: top;
    margin-top: 8px;
    width: 22px;
    height: 12px;
    background-size: 22px 12px;
    background-repeat: no-repeat;
}

.header .bulletin-wrapper .bulletin-text {
    vertical-align: top;
    margin: 0 4px;
    font-size: 10px;
}

.header .bulletin-wrapper .icon {
    position: absolute;
    font-size: 20px;
    right: 12px;
    top: 0px;
}

.header .background {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: -1;
    filter: blur(10px);
}

.header .background .detail {
    position: fixed;
    z-index: 100;
    top: 0;
    right: 0;
    width: 100%;
    height: 100%;
    overflow: auto;
    background: rgba(7,17,27,0.8);
    -webkit-backdrop-filter: blur(10px);
}

.tab {
    display: -ms-flexbox;
    display: flex;
    width: 100%;
    height: 40px;
    line-height: 40px;
    position: relative;
}

.tab .tab-item {
    flex: 1;
    text-align: center;
}

.tab .tab-item a {
    display: block;
    font-size: 14px;
    color: #4d555d;
}

.tab .tab-item .active {
    color: #f01414;
}

.list-mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 40;
    -webkit-backdrop-filter: 1;
    background: rgba(0,0,0,0.5);
}

.list-mask :fade-enter-active,:fade-leave-active {
    opacity: 1;
    transition: all 0.5s;
    background: rgba(7,17,27,0.6);
}

.goods-list :fade-enter,:fade-leave-active {
    opacity: 0;
    background: rgba(7,17,27,0);
}

.property-popup {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 51;
}

.popup-mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    z-index: 5;
}

.popup-contents {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    background-color: #fff;
    z-index: 6;
}

.pop-goods-info {
    display: flex;
    flex-direction: row;
    align-items: center;
    margin-left: 30rpx;
    padding: 30rpx 0;
    border-bottom: 1px solid #eee;
}

.pop-img-box {
    width: 120rpx;
    height: 120rpx;
    overflow: hidden;
    margin-right: 26rpx;
}

.pop-img-box .goods-thumbnail {
    width: 120rpx;
    height: 120rpx;
}

.pop-goods-title {
    width: 484rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    font-size: 26rpx;
    color: #000000;
}

.pop-goods-price {
    font-size: 26rpx;
    color: #f44;
    margin-top: 20rpx;
}

.pop-goods-close {
    width: 36rpx;
    height: 36rpx;
    flex: 1;
    background: url("http://qn.zxsite.cn/zxsite_shop/popup-close.png") no-repeat center center;
    background-size: 36rpx auto;
    align-self: flex-start;
    margin-top: -10rpx;
}

.size-label-box .label {
    font-size: 26rpx;
    color: #000;
    padding-left: 30rpx;
    padding: 30rpx 0 20rpx 30rpx;
}

.size-label-box .label-item-box {
    display: flex;
    margin-left: 30rpx;
    flex-direction: row;
    flex-wrap: wrap;
}

.size-label-box .label-item {
    font-size: 26rpx;
    color: #000;
    padding: 14rpx 20rpx;
    border: 1px solid #ddd;
    border-radius: 6rpx;
    margin: 0 20rpx 20rpx 0;
}

.size-label-box .label-item.active {
    color: #f44;
    border: 1px solid #f44;
}

.buy-num-box {
    display: flex;
    justify-content: space-between;
    padding: 30rpx 30rpx 48rpx 0;
    margin-left: 30rpx;
    border-top: 1px solid #eee;
    margin-top: 30rpx;
    align-items: center;
}

.num-label {
    font-size: 26rpx;
    color: #000000;
}

.buy-num-box .num-box {
    display: flex;
}

.buy-num-box .num-box .num-jian,.buy-num-box .num-box .num-input,.buy-num-box .num-box .num-jia {
    width: 80rpx;
    height: 64rpx;
    line-height: 62rpx;
    text-align: center;
    border: 1px solid #eee;
}

.buy-num-box .num-box .num-input {
    font-size: 28rpx;
}

.buy-num-box .num-box .num-input input {
    height: 100%;
}

.popup-join-btn {
    width: 100%;
    height: 100rpx;
    text-align: center;
    line-height: 100rpx;
    font-size: 30rpx;
    font-weight: bold;
    color: #ffffff;
    background-color: #f44;
}

.buy-num-box .num-box .hui {
    background-color: #f5f5f9;
}

.diy-form-wrapper {
    margin-top: 30rpx;
    margin-left: 30rpx;
}

.diy-form-item-wrapper {
    border-top: 1px solid #eee;
    padding: 30rpx 0;
    width: 720rpx;
    display: flex;
    font-size: 26rpx;
}

.diy-form-item-wrapper:last-child {
    padding-bottom: 0;
}

.diy-form-item-label {
    width: 180rpx;
    display: flex;
    align-items: center;
}

.diy-form-item-label.required:before {
    content: '*';
    color: #f44;
    margin-right: 8rpx;
}

.diy-form-item-value {
    width: 510rpx;
    display: flex;
    align-items: center;
    flex-wrap: wrap;
}

.diy-form-item-value .checkbox-wrapper {
    display: flex;
    align-items: center;
    margin-right: 5px;
    height: 50rpx;
}

.diy-form-item-value icon {
    width: auto;
    height: auto;
    margin-right: 5px;
}

.diy-form-item-value picker input,.diy-form-item-value picker {
    width: 100%;
}

.uploader-file {
    margin: 2px 5px 2px 0;
    position: relative;
    width: 79px;
    height: 79px;
}

.uploader-img {
    display: block;
    width: 79px;
    height: 79px;
}

.uploader-file-cancel {
    position: absolute;
    bottom: 0;
    color: #fff;
    background-color: #F43530;
    text-align: center;
    width: 158rpx;
    height: 50rpx;
    line-height: 50rpx;
    opacity: 0.8;
}

.uploader-box {
    margin: 2px 0;
    position: relative;
    width: 158rpx;
    height: 158rpx;
    border: 1px solid #d9d9d9;
}

.uploader-box:after,.uploader-box:before {
    content: " ";
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    background-color: #d9d9d9;
}

.uploader-box:before {
    width: 2px;
    height: 39.5px;
}

.uploader-box:after {
    width: 39.5px;
    height: 2px;
}

.uploader-box:active {
    border-color: #999;
}

.uploader-box:active:after,.uploader-box:active:before {
    background-color: #999;
}

.uploader-input {
    position: absolute;
    z-index: 1;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
}