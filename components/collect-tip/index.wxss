.collect-tip {
    max-width: 600rpx;
    position: fixed;
    top: 7px;
    right: 15px;
    z-index: 100;
    background: #fff;
    border-radius: 4px;
    padding: 10px 26px 10px 10px;
    box-shadow: 0 2px 8px rgba(0,0,0,.3);
}

.collect-tip:before {
    content: "";
    position: absolute;
    border: 7px solid transparent;
    border-bottom-color: #fff;
    top: -14px;
    right: 49px;
}

.collect-tip .van-icon-clear {
    position: absolute;
    font-size: 16px;
    line-height: 20px;
    color: #e6e6e6;
    top: 11px;
    right: 10px;
}

.collect-tip .share-icon {
    display: inline;
    font-size: 20px;
    line-height: 12px;
    position: relative;
    top: 4px;
}

.collect-tip__line {
    font-size: 14px;
    line-height: 20px;
    color: #333;
}

@font-face {
    font-family:wsc;src:url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),url(data:application/font-woff;charset=utf-8;base64,) format('woff');
}

.icon-collect-gift-share-icon:before {
    font-family: wsc;
    font-style: normal;
    font-weight: 400;
    content: "\f104";
}