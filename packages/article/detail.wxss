@import "../../templates/goods.wxss";

@import "../../templates/video.wxss";

@import "../../templates/image.wxss";

@import "../../templates/form.wxss";

@import "../../templates/line.wxss";

@import "../../templates/white.wxss";

page {
    background-color: #fff;
}

.title {
    padding: 25rpx;
    font-size: 22px;
    line-height: 1.4;
}

.meta,.bottom {
    padding: 25rpx;
    font-size: 15px;
    color: #8c8c8c;
}

.meta {
    padding-top: 0;
}

.bottom {
    display: flex;
    justify-content: space-between;
}

.icon-praise {
    background: url(data:image/png;base64,) no-repeat 0 0;
    font-size: 16px;
    width: 1em;
    height: 1em;
    background-size: 1em;
    margin-right: 5px;
    vertical-align: 0;
    position: relative;
    top: 1px;
}

.icon-praised {
    background-position-y: -1.3em;
}

.goods-box.list-style-3 {
    padding: 0;
}