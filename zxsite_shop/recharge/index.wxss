.banner {
    width: 750rpx;
    height: 312rpx;
    margin-bottom: 10rpx;
}

.money-wrap {
    width: 750rpx;
    display: flex;
    flex-wrap: wrap;
    padding: 10rpx 0;
}

.money {
    margin-top: 20rpx;
    margin-left: 30rpx;
    width: 208rpx;
    font-size: 32rpx;
    text-align: center;
    border: 1px solid #f44;
    border-radius: 8rpx;
    color: #f44;
    padding: 20rpx 0;
}

.money text {
    font-size: 22rpx;
    display: block;
    margin-top: 8rpx;
}

.money.active {
    color: #fff;
    background-color: #f44;
}

button {
    width: 690rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    margin-top: 30rpx;
    margin-bottom: 30rpx;
    border-radius: 6rpx;
    box-sizing: border-box;
    background-color: #f44;
    color: #fff;
}

.log {
    color: #586b95;
    font-size: 26rpx;
}

.log text {
    margin: 0 20rpx;
}