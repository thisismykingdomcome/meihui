<van-tabs bind:change="onTabChange">
    <van-tab title="基本信息">
        <van-cell-group customClass="form-wrapper">
            <van-field bind:change="onFieldChange" border="true" id="nickname" label="昵称" placeholder="请输入昵称" value="{{member.nickname}}"></van-field>
            <van-cell isLink title="会员等级">
                <picker bindchange="onLevelPickerChange" range="{{levelList}}" rangeKey="name" value="{{levelIndex}}">
                    <view>
            {{levelText}}
          </view>
                </picker>
            </van-cell>
            <van-field bind:change="onFieldChange" border="true" id="realname" label="真实姓名" placeholder="请输入真实姓名" value="{{member.realname}}"></van-field>
            <van-cell isLink bind:click="onGenderClick" title="性别" value="{{genderText}}"></van-cell>
            <van-cell isLink bind:click="onBirthdayClick" title="出生生日" value="{{member.birthday?member.birthday:'请选择'}}"></van-cell>
            <van-field bind:change="onFieldChange" border="true" id="address" label="详细地址" placeholder="请输入详细地址" value="{{member.address}}"></van-field>
            <van-field bind:change="onFieldChange" border="true" id="mobile" label="手机号码" placeholder="请输入手机号码" type="number" value="{{member.mobile}}"></van-field>
            <van-field bind:change="onFieldChange" border="true" id="qq" label="QQ号" placeholder="请输入QQ号" type="number" value="{{member.qq}}"></van-field>
            <van-field bind:change="onFieldChange" border="true" id="email" label="电子邮箱" placeholder="请输入电子邮箱" value="{{member.email}}"></van-field>
        </van-cell-group>
    </van-tab>
    <van-tab title="联系方式">
        <van-cell-group customClass="form-wrapper">
            <van-field bind:change="onFieldChange" border="true" id="telephone" label="固定电话" placeholder="请输入固定电话" type="number" value="{{member.telephone}}"></van-field>
            <van-field bind:change="onFieldChange" border="true" id="msn" label="MSN" placeholder="请输入MSN" value="{{member.msn}}"></van-field>
            <van-field bind:change="onFieldChange" border="true" id="taobao" label="阿里旺旺" placeholder="请输入阿里旺旺" value="{{member.taobao}}"></van-field>
            <van-field bind:change="onFieldChange" border="true" id="alipay" label="支付宝帐号" placeholder="请输入支付宝帐号" value="{{member.alipay}}"></van-field>
        </van-cell-group>
    </van-tab>
    <van-tab title="教育情况">
        <van-cell-group customClass="form-wrapper">
            <van-field bind:change="onFieldChange" border="true" id="studentid" label="学号" placeholder="请输入学号" value="{{member.studentid}}"></van-field>
            <van-field bind:change="onFieldChange" border="true" id="grade" label="班级" placeholder="请输入班级" value="{{member.grade}}"></van-field>
            <van-field bind:change="onFieldChange" border="true" id="graduateschool" label="毕业学校" placeholder="请输入毕业学校" value="{{member.graduateschool}}"></van-field>
            <van-cell isLink title="学历">
                <picker bindchange="onEducationPickerChange" range="{{educationList}}" value="{{educationIndex}}">
                    <view>
            {{member.education?member.education:'请选择'}}
          </view>
                </picker>
            </van-cell>
        </van-cell-group>
    </van-tab>
    <van-tab title="工作情况">
        <van-cell-group customClass="form-wrapper">
            <van-field bind:change="onFieldChange" border="true" id="company" label="公司" placeholder="请输入公司" value="{{member.company}}"></van-field>
            <van-field bind:change="onFieldChange" border="true" id="occupation" label="职业" placeholder="请输入职业" value="{{member.occupation}}"></van-field>
            <van-field bind:change="onFieldChange" border="true" id="position" label="职位" placeholder="请输入职位" value="{{member.position}}"></van-field>
            <van-field bind:change="onFieldChange" border="true" id="revenue" label="年收入" placeholder="请输入年收入" type="number" value="{{member.revenue}}"></van-field>
        </van-cell-group>
    </van-tab>
    <van-tab title="个人情况">
        <van-cell-group customClass="form-wrapper">
            <van-cell isLink title="星座">
                <picker bindchange="onConstellationPickerChange" range="{{constellationList}}" value="{{constellationIndex}}">
                    <view>
            {{member.constellation?member.constellation:'请选择'}}
          </view>
                </picker>
            </van-cell>
            <van-cell isLink title="生肖">
                <picker bindchange="onZodiacPickerChange" range="{{zodiacList}}" value="{{zodiacIndex}}">
                    <view>
            {{member.zodiac?member.zodiac:'请选择'}}
          </view>
                </picker>
            </van-cell>
            <van-field bind:change="onFieldChange" border="true" id="nationality" label="国籍" placeholder="请输入国籍" value="{{member.nationality}}"></van-field>
            <van-field bind:change="onFieldChange" border="true" id="height" label="身高" placeholder="请输入身高" value="{{member.height}}"></van-field>
            <van-field bind:change="onFieldChange" border="true" id="weight" label="体重" placeholder="请输入体重" value="{{member.weight}}"></van-field>
            <van-cell isLink title="血型">
                <picker bindchange="onBloodtypePickerChange" range="{{bloodtypeList}}" value="{{bloodtypeIndex}}">
                    <view>
            {{member.bloodtype?member.bloodtype:'请选择'}}
          </view>
                </picker>
            </van-cell>
            <van-field bind:change="onFieldChange" border="true" id="idcard" label="身份证号" placeholder="请输入身份证号" value="{{member.idcard}}"></van-field>
            <van-field bind:change="onFieldChange" border="true" id="zipcode" label="邮编" placeholder="请输入邮编" type="number" value="{{member.zipcode}}"></van-field>
            <van-field bind:change="onFieldChange" border="true" id="site" label="个人网站" placeholder="请输入个人网站" value="{{member.site}}"></van-field>
            <van-field bind:change="onFieldChange" border="true" id="affectivestatus" label="情感状态" placeholder="请输入情感状态" value="{{member.affectivestatus}}"></van-field>
            <van-field bind:change="onFieldChange" border="true" id="bio" label="交友目的" placeholder="请输入交友目的" value="{{member.bio}}"></van-field>
            <van-field bind:change="onFieldChange" border="true" id="interest" label="兴趣爱好" placeholder="请输入兴趣爱好" value="{{member.interest}}"></van-field>
        </van-cell-group>
    </van-tab>
</van-tabs>
<container-block customClass="form-bottom">
    <van-row>
        <van-col span="24">
            <van-button bind:click="onSave" block="true" type="primary">保存</van-button>
        </van-col>
    </van-row>
</container-block>
<van-dialog showCancelButton useSlot bind:cancel="onLabelDialogCancel" bind:confirm="onLabelDialogConfirm" show="{{labelDialogShow}}" title="选择标签">
    <van-checkbox-group bind:change="onLabelChange" value="{{labelValue}}">
        <van-cell-group customClass="label-group">
            <van-cell clickable bind:click="onLabelToggle" data-name="{{item.id}}" title="{{item.name}}" wx:for="{{labelList}}" wx:key="{{index}}">
                <van-checkbox catch:tap="noop" checkedColor="#4b0" class="label-{{item.id}}" name="{{item.id}}"></van-checkbox>
            </van-cell>
        </van-cell-group>
    </van-checkbox-group>
</van-dialog>
<van-action-sheet actions="{{genderActions}}" bind:cancel="onGenderActionsCancel" bind:close="onGenderActionsCancel" bind:select="onGenderActionsSelect" cancelText="取消" show="{{genderActionsShow}}"></van-action-sheet>
<van-popup position="bottom" show="{{birthdayPopupShow}}">
    <van-datetime-picker bind:cancel="onBirthdayCancel" bind:change="onBirthdayChange" bind:confirm="onBirthdayConfirm" minDate="{{minDate}}" type="date" value="{{currentDate}}"></van-datetime-picker>
</van-popup>
