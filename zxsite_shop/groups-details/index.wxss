.uploader-file-cancel {
    position: absolute;
    bottom: 0;
    color: #fff;
    background-color: #F43530;
    text-align: center;
    width: 158rpx;
    height: 50rpx;
    line-height: 50rpx;
    opacity: 0.8;
}

.uploader-box {
    margin: 2px 0;
    position: relative;
    width: 158rpx;
    height: 158rpx;
    border: 1px solid #d9d9d9;
}

.uploader-box:after,.uploader-box:before {
    content: " ";
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    background-color: #d9d9d9;
}

.uploader-box:before {
    width: 2px;
    height: 39.5px;
}

.uploader-box:after {
    width: 39.5px;
    height: 2px;
}

.uploader-box:active {
    border-color: #999;
}

.uploader-box:active:after,.uploader-box:active:before {
    background-color: #999;
}

.uploader-input {
    position: absolute;
    z-index: 1;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
}

